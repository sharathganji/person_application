<div class="col-xs-12">
  <div class="header">
    <h3>Hash Of Managers</h3>
  </div>
  <div class="body table-responsive">
    <table class="table table-bordered">

      <thead>
      <tr>
        <th>ManagerName</th>
        <th>Person Count</th>
        <th>City Name</th>
      </tr>
      </thead>
      <tbody>
      <% @person_hash.each do |key, value| %>
          <tr>
            <td><%= key %></td>
            <% value.each do |key1, value1| %>
            <td><%= value1 %></td>
                <% end %>
          </tr>
      <% end %>
      </tbody>
    </table>
  </div>

</div>


